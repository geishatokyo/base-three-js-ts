<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <title><%= htmlWebpackPlugin.options.title %></title>
  </head>

  <body>
    <div id="canvas-frame">
      <div id="ui-frame">
        <svg id="ui-svg" viewBox="0 0 540 1080">
          <symbol id="svg-loading-rolling" viewBox="0 0 100 100">
            <path
              fill="#fff"
              d="M73,50c0-12.7-10.3-23-23-23S27,37.3,27,50 M30.9,50c0-10.5,8.5-19.1,19.1-19.1S69.1,39.5,69.1,50"
            ></path>
          </symbol>
          <symbol id="finger" viewBox="0 0 30 30">
            <image
              width="30"
              height="30"
              xlink:href="data:image/png;base64,"
              transform="rotate(-45, 15, 15)"
            ></image>
          </symbol>
          <path d="M 420 400 l 50 0" stroke="none" fill="none" id="path-done" />
          <g id="tutorial1" transform="translate(350,650)">
            <g class="pound-animation">
              <use
                class="pound-animation"
                xlink:href="#finger"
                width="60"
                height="60"
              ></use>
            </g>
          </g>
          <g id="fail-group" visibility="hidden" transform="translate(185,605)">
            <image id="retry-button" width="184.5" height="100"></image>
          </g>
          <image id="icon" x="420" y="900" width="120" height="120" />
          <g id="next-group" visibility="hidden">
            <g transform="translate(50,100)">
              <image id="complete-text" width="481.5" height="150" />
            </g>
            <g transform="translate(185,605)">
              <image id="next-button" width="184.5" height="100" />
            </g>
          </g>
          <g transform="translate(270,965)">
            <g>
              <image id="install" x="-100" y="-25" width="200" height="70" />
            </g>
          </g>
          <g id="loading">
            <rect
              x="-1000"
              y="-1000"
              width="3000"
              height="3000"
              fill="#000"
            ></rect>
            <g class="rotate-animation">
              <g transform="translate(220,490)">
                <use
                  xlink:href="#svg-loading-rolling"
                  width="100"
                  height="100"
                ></use>
              </g>
            </g>
          </g>
        </svg>
      </div>
      <canvas id="main-scene"> </canvas>
    </div>
  </body>
</html>
